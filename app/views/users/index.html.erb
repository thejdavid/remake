
<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Artist list</h1>
  </div>
    <div class="row">
    <% @artist.each do |artist| %>
    <div class="col-lg-3 col-md-4 col-xs-6 thumb">
      <a class="thumbnail" href=<%= "users/"+ artist.id.to_s%>>
        <%=image_tag artist.picture.url(:thumb) %>
        <p style="text-align:center"> <%= artist.username %></p>
      </a>
    </div>
    <% end %>
  </div>
</div>

